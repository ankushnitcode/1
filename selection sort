#include <bits/stdc++.h>
using namespace std ;
void  selectionsort(vector <int> &arr,int size){
    for (int i=0;i<size;i++){
        int min =i;
     for(int j=i+1;j<size;j++){
              if(arr[j]<arr[min]){
                  min=j;
              }}
              int temp= arr[i];
              arr[i]=arr[min];
              arr[min]=temp;

     }
     
    }
int main(){
    vector<int> arr{1,3,5,2};
    selectionsort(arr,4);
    for(int i=0;i<4;i++){
        cout<<arr[i]<<" ";
    }
}
